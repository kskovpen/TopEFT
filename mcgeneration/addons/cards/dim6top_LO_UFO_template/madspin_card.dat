set ms_dir ./madspingrid

set Nevents_for_max_weigth 250
set max_weight_ps_point 400

set max_running_process 1

import model dim6top_LO_UFO

define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

decay t > w+ b DIM6<=1 FCNC=0, w+ > l+ vl DIM6<=1 FCNC=0
decay t~ > w- b~ DIM6<=1 FCNC=0, w- > l- vl~ DIM6<=1 FCNC=0

#decay t > w+ b, w+ > ell+ vl
#decay t~ > w- b~, w- > ell- vl~
#decay w+ > ell+ vl
#decay w- > ell- vl~

launch
